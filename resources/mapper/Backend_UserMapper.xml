<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sq.appinfo.dao.Backend_Userdao">
	<!-- 根据参数查询backend_user表的数据 -->
	<select id="getByuserCodeAnduserPassword" resultType="Backend_User">
		SELECT * FROM backend_user WHERE userCode=#{userCode} and userPassword=#{userPassword}
	</select>

</mapper>
